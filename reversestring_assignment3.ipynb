    "def reverse_string(str):\n",
    "    str1 = \"\"\n",
    "    for i in str:\n",
    "        str1 = i + str1\n",
    "    return str1\n",
    "str = input(\"enter the string that need to be reversed : \")\n",
    "print(\"the original string is: \",str)\n",
    "print(\"the reverse string is\",reverse_string(str))\n",
